
   <div class="container withGutter">
            <div class="three columns">
                    <img src="/assets/animals/lambchop.png" alt="test image" id="animalPic">
            </div>
            
            <div id="animalInfo" class="nine columns">
                <div>
                <h1>{{cleanAnimal.type}}</h1>
                    <ul>
                        <li><strong>Name:</strong> <span id="nameInfo">{{cleanAnimal.name}}</span></li>
                        <li><strong>Breed:</strong> <span id="breedInfo">{{cleanAnimal.breed}}</span></li>
                        <li><strong>Food(organic):</strong> <span id="orgFood">${{cleanAnimal.food_manufactured}}</span></li>
                        <li><strong>Food(Manufactured):</strong> <span id="manFood">${{cleanAnimal.food_organic}}</span></li>
                    </ul>
                </div>
                <div id="animalActions">
                    <button class="button-primary">Edit Animal</button>
                    <button class="button">Delete Animal</button>
                </div>
            </div>
        </div>
